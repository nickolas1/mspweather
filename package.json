{
  "name": "mspweather",
  "version": "0.0.0",
  "main": "index.js",
  "engines": {
    "node": "6.9.1"
  },
  "scripts": {
    "prebuild:css": "node-sass client/js/style.scss client/js/style.css",
    "css": "npm run prebuild:css && postcss --use autoprefixer -b 'last 2 versions' < client/js/style.css | postcss --use cssnano > client/js/style.min.css",
    "css:w": "nodemon -e scss -x \"npm run css\"",
    "babel": "babel server --out-dir lib",
    "babel:w": "babel -w server --out-dir lib",
    "webpack": "webpack",
    "webpack:w": "webpack --watch",
    "postinstall": "npm run babel && npm run css && npm run webpack",
    "start": "node lib/app"
  },
